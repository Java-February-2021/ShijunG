public class Bat extends Mammal {
	
	public Bat() {
	this.energyLevel = 300;
	}
	
	public void fly() {
		energyLevel -= 50;
		System.out.println("I am a flying bat!");
	}
	public void eatHumans() {
		energyLevel += 25;
		System.out.println("I am eating human!");
	}
	public void attackTown() {
		energyLevel -= 100;
		System.out.println("I am attacking!");
	}
}